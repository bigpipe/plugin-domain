  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigpipe/plugin-domain/issues"
  },
  "homepage": "https://github.com/bigpipe/plugin-domain",
  "dependencies": {
    "diagnostics": "~1.0.1",
    "lodash.merge": "~3.3.2"
  },
  "devDependencies": {
    "assume": "~1.2.5",
    "bigpipe": "~0.9.12",
    "istanbul": "~0.3.0",
    "mocha": "~2.2.0",
    "pre-commit": "~1.1.1"
  }

{
  "name": "bigpipe-domain",
  "version": "1.2.0",
  "description": "Scope the resources from the compiler to a domain",
  "main": "index.js",
  "scripts": {
    "100%": "istanbul check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "test": "mocha ./test.js",
